{
  "name": "divera247-api-unofficial",
  "version": "0.0.0-development",
  "description": "Unofficial Divera 24/7 API Wrapper",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "build:release": "rm -fr dist/* && tsup src/index.ts --format esm,cjs --minify --dts src/index.ts",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "divera",
    "divera 24/7",
    "divera api"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "node": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timonmasberg/divera247.git"
  },
  "author": "Timon Masberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timonmasberg/divera247/issues"
  },
  "homepage": "https://github.com/timonmasberg/divera247#readme",
  "dependencies": {
    "axios": "^1.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/axios": "0.14.0",
    "@types/jest": "^29.5.1",
    "@types/node": "18.17.16",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "14.0.1",
    "nodemon": "3.0.1",
    "prettier": "2.8.8",
    "semantic-release": "^21.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsup": "^7.0.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.js": "eslint --cache --fix"
  }
}
