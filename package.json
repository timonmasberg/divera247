{
  "name": "divera247-api-unofficial",
  "version": "0.0.0-development",
  "description": "Unofficial Divera 24/7 API Wrapper",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "build:release": "rm -fr dist/* && tsup src/index.ts --format esm,cjs --minify --dts src/index.ts",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "divera",
    "divera 24/7",
    "divera api"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "node": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timonmasberg/divera247.git"
  },
  "author": "Timon Masberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timonmasberg/divera247/issues"
  },
  "homepage": "https://github.com/timonmasberg/divera247#readme",
  "dependencies": {
    "axios": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@types/axios": "0.14.4",
    "@types/jest": "^30.0.0",
    "@types/node": "22.18.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "^9.0.0",
    "jest": "^30.0.0",
    "lint-staged": "16.1.5",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.2",
    "tsup": "^8.0.0",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.js": "eslint --cache --fix"
  }
}
